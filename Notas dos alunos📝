alunos = ['Gabriel', 'Henrique', 'Lucas', 'Arthur', 'Felipe', 'Antônio', 'Nicolas', 'Davi', 'Luis', 'Victor']
notasN1 = [9.0, 6.5, 7.0, 3.5, 6.5, 8.0, 9.5, 10.0, 4.5, 7.0]
notasN2 = [5.0, 4.5, 8.5, 10.0, 2.5, 5.5, 7.5, 3.5, 1.5, 8.5]
for i in range(len(alunos)):
    media = (notasN1[i] + notasN2[i]) / 2
    
    print(f"{alunos[i]} - Média: {media}")
    
    if media >= 6:
        print("Aprovado!")
    else:
        print("Reprovado!")
        
    print("-" * 20)
